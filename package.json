  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chriskalmar/lost-pixel-action/issues"
  },
  "homepage": "https://github.com/chriskalmar/lost-pixel-action#readme",
  "dependencies": {
    "async": "^3.2.3",
    "axios": "^0.24.0",
    "dotenv": "^10.0.0",
    "lodash.kebabcase": "^4.1.1",
    "minio": "^7.0.25",
    "pixelmatch": "^5.2.1",
    "playwright": "^1.20.1",
    "pngjs": "^6.0.0"
  },
  "devDependencies": {
    "@octokit/webhooks-types": "^5.2.0",
    "@types/async": "^3.2.12",
    "@types/jest": "27.0.3",
    "@types/lodash.kebabcase": "^4.1.6",
    "@types/minio": "7.0.11",
    "@types/node": "17.0.4",
    "@types/pixelmatch": "^5.2.4",
    "@types/pngjs": "^6.0.1",
    "jest": "27.4.5",
    "rimraf": "3.0.2",
    "ts-jest": "27.1.2",
    "ts-node": "10.4.0",
    "typescript": "4.5.4"
  }
}
