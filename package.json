{
  "name": "lost-pixel-action",
  "version": "1.0.0",
  "description": "GitHub Action for Lost Pixel integration",
  "main": "index.js",
  "scripts": {
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "build": "rimraf dist && tsc",
    "build-example-storybook": "rimraf examples/storybook-demo/storybook-static && cd examples/storybook-demo && npm install && npm run build-storybook",
    "dev": "ts-node -r ./loadEnv src/",
    "start": "NODE_ENV=production node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chriskalmar/lost-pixel-action.git"
  },
  "keywords": [],
