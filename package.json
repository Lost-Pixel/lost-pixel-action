{
  "name": "lost-pixel-action",
  "version": "1.3.0",
  "description": "GitHub Action for Lost Pixel integration",
  "main": "dist/",
  "bin": "dist/bin.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "build": "rimraf dist && tsc",
    "build-example-storybook": "rimraf examples/storybook-demo/storybook-static && cd examples/storybook-demo && npm install && npm run build-storybook",
    "lint": "tsc --noEmit && eslint '*/**/*.ts'",
    "lint-fix": "tsc --noEmit && eslint '*/**/*.ts' --fix",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "release": "np --no-publish",
    "dev": "ts-node src/runner",
    "start": "NODE_ENV=production node dist/runner.js"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "./node_modules/eslint/bin/eslint.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chriskalmar/lost-pixel-action.git"
  },
  "keywords": [],
  "author": "Chris Kalmar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chriskalmar/lost-pixel-action/issues"
  },
  "homepage": "https://github.com/chriskalmar/lost-pixel-action#readme",
  "dependencies": {
    "async": "^3.2.3",
    "axios": "^0.26.1",
    "lodash.get": "^4.4.2",
    "lodash.kebabcase": "^4.1.1",
    "minio": "^7.0.26",
    "pixelmatch": "^5.2.1",
    "playwright": "^1.20.1",
    "pngjs": "^6.0.0",
    "ts-node": "^10.7.0"
  },
  "devDependencies": {
    "@octokit/webhooks-types": "^5.2.0",
    "@types/async": "^3.2.12",
    "@types/jest": "27.0.3",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.kebabcase": "^4.1.6",
    "@types/minio": "7.0.11",
    "@types/pixelmatch": "^5.2.4",
    "@types/pngjs": "^6.0.1",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "jest": "27.4.5",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.2",
    "rimraf": "3.0.2",
    "ts-jest": "27.1.2",
    "typescript": "4.5.4"
  }
}
